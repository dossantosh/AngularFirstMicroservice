{
  "name": "UsersManagement",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "apps/UsersManagement/src",
  "targets": {
    "build": {
      "executor": "@nx/angular:webpack-browser",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/apps/UsersManagement",
        "index": "apps/UsersManagement/src/index.html",
        "main": "apps/UsersManagement/src/main.ts",
        "tsConfig": "apps/UsersManagement/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          {
            "glob": "**/*",
            "input": "shell/public"
          },
          {
            "glob": "**/*",
            "input": "libs/assets/src/lib/assets",
            "output": "assets"
          },
          {
            "glob": "favicon.ico",
            "input": "libs/assets/src/lib/assets",
            "output": "/"
          }
        ],
        "stylePreprocessorOptions": {
          "includePaths": [
            "libs/styles/src/lib"
          ]
        },
        "styles": [
          "libs/styles/src/lib/common.styles.scss",
          "libs/styles/src/lib/theme.scss"
        ],
        "customWebpackConfig": {
          "path": "apps/UsersManagement/webpack.config.ts"
        }
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "4kb",
              "maximumError": "8kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "continuous": true,
      "executor": "@nx/angular:dev-server",
      "options": {
        "port": 4201,
        "publicHost": "http://localhost:4201",
        "buildTarget": "UsersManagement:build:development"
      },
      "configurations": {
        "production": {
          "buildTarget": "UsersManagement:build:production"
        },
        "development": {
          "buildTarget": "UsersManagement:build:development"
        }
      },
      "defaultConfiguration": "development"
    }
  }
}